# echo "=== .zprofile START ==="

if [ "$SYSTEM_TYPE" = "Linux" ]; then
    if [ "$OS_ID" = "arch" ]; then
        command -v base16_hopscotch &>/dev/null && base16_hopscotch # gray-brown-ish
    else
    	command -v base16_materia &>/dev/null && base16_materia # blue-ish
    fi
elif [ "$SYSTEM_TYPE" = "Darwin" ]; then
      command -v base16_material-palenight &>/dev/null && base16_material-palenight # light purple-ish
fi

